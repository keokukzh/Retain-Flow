{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "DATABASE_URL": "@database-url",
    "JWT_SECRET": "@jwt-secret",
    "STRIPE_API_KEY": "@stripe-api-key",
    "STRIPE_PUBLISHABLE_KEY": "@stripe-publishable-key",
    "DISCORD_BOT_TOKEN": "@discord-bot-token",
    "EMAIL_SERVICE_API_KEY": "@email-service-api-key",
    "REDIS_URL": "@redis-url",
    "NEXT_PUBLIC_POSTHOG_KEY": "@posthog-key",
    "NEXT_PUBLIC_POSTHOG_HOST": "@posthog-host",
    "POSTHOG_PERSONAL_API_KEY": "@posthog-personal-key",
    "N8N_API_URL": "@n8n-api-url",
    "N8N_API_KEY": "@n8n-api-key",
    "COMPOSIO_API_KEY": "@composio-api-key",
    "CHATWOOT_API_URL": "@chatwoot-api-url",
    "CHATWOOT_API_TOKEN": "@chatwoot-api-token",
    "NEXT_PUBLIC_CHATWOOT_WEBSITE_TOKEN": "@chatwoot-website-token",
    "UNLOCK_NETWORK": "@unlock-network",
    "UNLOCK_PROVIDER_URL": "@unlock-provider-url"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/webhooks/(.*)",
      "destination": "/api/webhooks/$1"
    }
  ]
}
